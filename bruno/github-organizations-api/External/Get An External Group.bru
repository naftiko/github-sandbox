meta {
  name: Get An External Group
  type: http
  seq: 1
}

get {
  url: {{baseUrl}}/orgs/:org/external-group/:group_id
  body: none
  auth: inherit
}

params:path {
  org: {{owner}}
  group_id: 1
}

